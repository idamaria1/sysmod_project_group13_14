@model SysModelBank.Areas.Admin.Models.LogOverview.LogList

<link rel="stylesheet" href="~/css/adminLogOverView.css">

<div class="container myContainer">
    <div class="row">
        <div class="col-12">
            <h1 class="mt-3 mb-3">System Log</h1>
        </div>
    </div>

    <table class="myTable mt-4">
        <thead>
            <tr>
                <th>ID:</th>
                <th>Time:</th>
                <th>Origin:</th>
                <th>Message:</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var log in Model.Logs)
            {
                <tr>
                    <th>@log.Id</th>
                    <td><b>@log.time</b></td>
                    <td><b>@log.origin</b></td>
                    <td><b>@log.value</b></td>
                </tr>
            }
        </tbody>
    </table>
</div>
